    "import csv\n",
    "import re\n",
    "\n",
    "isbn_pattern = re.compile(r\"^\\d{13}$\")\n",
    "\n",
    "with open(\"example.csv\") as csvfile:\n",
    "    csvreader = csv.reader(csvfile)\n",
    "    for row in csvreader:\n",
    "        for col in row:\n",
    "            if isbn_pattern.match(col):\n",
    "                print(col)"
